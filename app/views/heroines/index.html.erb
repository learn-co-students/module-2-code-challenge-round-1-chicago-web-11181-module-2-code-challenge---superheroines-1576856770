<h2>All Heroines</h2>
<br>
<h3>Search by Powah!</h3>
<%= form_tag(powers_path method: :get) do %>
  Power <%= text_field_tag :power %>
  <%= submit_tag 'Search' %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
<br>
<p><%= link_to "Create a Heroine!", new_heroine_path %>
<p><%= link_to "POWER INDEX", powers_path %></p>